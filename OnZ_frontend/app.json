{
  "expo": {
    "name": "OnZ",
    "slug": "OnZ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/commons/OnZ_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "width": 100
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
        "expo-font"      
    ]
  }
}
